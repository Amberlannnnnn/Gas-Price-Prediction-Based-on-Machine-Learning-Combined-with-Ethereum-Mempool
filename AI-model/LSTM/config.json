{
    "data": {
        "filename": "data-5.csv",
        "columns": [
            "middlle_gp",
            "avg_gp",
            "base_fee_per_gas"
        ],
        "sequence_length": 50,
        "train_test_split": 0.75,
        "normalise": true
    },
    "training": {
        "epochs": 15,
        "batch_size": 32
    },
    "model": {
        "loss": "mse",
        "optimizer": "adam",
        "save_dir": "saved_models",
        "layers": [{
                "type": "lstm",
                "neurons": 70,
                "input_timesteps": 49,
                "input_dim": 3,
                "return_seq": true
            },
            {
                "type": "dropout",
                "rate": 0.2
            },
            {
                "type": "lstm",
                "neurons": 70,
                "return_seq": true
            },
            {
                "type": "lstm",
                "neurons": 70,
                "return_seq": false
            },
            {
                "type": "dropout",
                "rate": 0.2
            },
            {
                "type": "dense",
                "neurons": 1,
                "activation": "linear"
            }
        ]
    }
}